<?xml version="1.0" ?>
<odoo>
    <record id="p_d_default_code_seq" model="ir.sequence">
        <field name="name">pickup and delivery Code Mandatory</field>
        <field name="code">pickup.delivery.code</field>
        <field name="prefix">PD-</field>
        <field name="padding">4</field>
