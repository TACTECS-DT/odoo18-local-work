<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="setu_inventory_count_sequence" model="ir.sequence">
            <field name="name">Inventory Count</field>
            <field name="code">setu.inventory.count.seq</field>
            <field name="prefix">INV/C/</field>
            <field name="padding">7</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="setu_inventory_count_session_sequence" model="ir.sequence">
            <field name="name">Inventory Count Session</field>
            <field name="code">setu.inventory.count.session.seq</field>
            <field name="prefix">INV/S/</field>
            <field name="padding">7</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>
