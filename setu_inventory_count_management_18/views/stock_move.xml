<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="view_sicm_stock_move_form" model="ir.ui.view">
        <field name="name">sicm.stock.move.form</field>
        <field name="model">stock.move</field>
        <field name="inherit_id" ref="stock.view_move_form"/>
        <field name="arch" type="xml">
            <field name="origin" position="after">
                <field name="inventory_count_id" 
                
                invisible="not inventory_count_id"/>
                <field name="inventory_adj_id" 
                
                invisible="not inventory_adj_id"/>
            </field>
        </field>
    </record>
</odoo>
